{
  "platform": {
    "apiRuntime": "dotnet-isolated:8.0"
  },
  "auth": {
    "identityProviders": {
      "github": {
        "registration": {
          "clientIdSettingName": "GITHUB_CLIENT_ID",
          "clientSecretSettingName": "GITHUB_CLIENT_SECRET"
        }
      },
      "customOpenIdConnectProviders": {
        "EntraId": {
          "registration": {
            "clientIdSettingName": "AZURE_AD_B2C_CLIENT_ID",
            "clientCredential": {
              "clientSecretSettingName": "AZURE_AD_B2C_CLIENT_SECRET"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://mynakedagility.ciamlogin.com/658c1a65-f834-4a61-b68c-91b9a7b3e481/v2.0/.well-known/openid-configuration"
            }
          },
          "login": {
            "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
            "scopes": [],
            "loginParameterNames": []
          }
        }
      }
    }
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "routes": []
}
