{
  "platform": {
    "apiRuntime": "dotnet-isolated:8.0"
  },
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://mynakedagility.ciamlogin.com/658c1a65-f834-4a61-b68c-91b9a7b3e481/v2.0/",
          "clientIdSettingName": "AZURE_AD_B2C_CLIENT_ID",
